import socket

def ip_range_to_ips(ip_range):
    start_ip, end_ip = ip_range.split('-')
    start_ip_int = int.from_bytes(socket.inet_aton(start_ip), byteorder='big')
    end_ip_int = int.from_bytes(socket.inet_aton(end_ip), byteorder='big')
    ip_list = []
    for ip_int in range(start_ip_int, end_ip_int+1):
        ip = socket.inet_ntoa(ip_int.to_bytes(4, byteorder='big'))
        ip_list.append(ip)
    return ip_list

input_file = input("Enter the path of the input file: ")
with open(input_file, 'r') as f:
    ip_ranges = f.read().splitlines()

ips = []
for ip_range in ip_ranges:
    ip_list = ip_range_to_ips(ip_range)
    ips.extend(ip_list)

for ip in ips:
    print(ip)
